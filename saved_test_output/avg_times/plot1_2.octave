function [newX,avgY,errY] = readplotdata(file)

M = load("-ascii", file);
X=M(:,1);
Y=M(:,2);

X/=1000;
Y/=1000;

# find equal X values and group them for averaging
newX=ones(length(X),1); # hack for the zero case
avgY=newX;
errY=newX;
max=1;
for i=1:length(X)
  found=0;
  for j=1:max
    if (X(i)==newX(j))
      found = 1;
      break;
    endif
  endfor
  if (found==0)
    tmpY = zeros(length(X),1);
    yptr = 1;
    num=0;
    for j=1:length(X)
      if (X(i)==X(j))
        tmpY(yptr) = Y(j);
        yptr++;
      endif
    endfor
    tmpY=tmpY(1:yptr-1);
    newX(max) = X(i);
    avgY(max) = mean(tmpY);
    errY(max) = std(tmpY,0);
    max++;
  endif
endfor
newX=newX(1:max-1);
avgY=avgY(1:max-1);
errY=errY(1:max-1);

#errorbar(newX,avgY,errY,fmt);
endfunction


clf;
hold on;
xlabel "Allocation size (kB)";
ylabel "Pause time (ms)";
title 'Time taken by GC\_malloc() averaged over 1000 consecutive allocations and 10 independent runs for every 1kB';
